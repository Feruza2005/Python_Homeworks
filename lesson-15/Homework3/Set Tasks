def union_of_sets(set1, set2):
    return set1 | set2

def intersection_of_sets(set1, set2):
    return set1 & set2

def difference_of_sets(set1, set2):
    return set1 - set2

def check_subset(set1, set2):
    return set1 <= set2

def check_element_in_set(s, elem):
    return elem in s

def set_length(s):
    return len(s)

def convert_list_to_set(lst):
    return set(lst)

def remove_element_from_set(s, elem):
    s.discard(elem)
    return s

def clear_set(s):
    s.clear()
    return s

def check_if_set_is_empty(s):
    return len(s) == 0

def symmetric_difference_of_sets(set1, set2):
    return set1 ^ set2

def add_element_to_set(s, elem):
    s.add(elem)
    return s

def pop_element_from_set(s):
    return s.pop() if s else None

def find_maximum_in_set(s):
    return max(s) if s else None

def find_minimum_in_set(s):
    return min(s) if s else None

def filter_even_numbers_from_set(s):
    return {x for x in s if x % 2 == 0}

def filter_odd_numbers_from_set(s):
    return {x for x in s if x % 2 != 0}

def create_set_of_range(start, end):
    return set(range(start, end + 1))

def merge_and_deduplicate(list1, list2):
    return set(list1 + list2)

def check_disjoint_sets(set1, set2):
    return set1.isdisjoint(set2)


def remove_duplicates_from_list(lst):
    return list(set(lst))

def count_unique_elements(lst):
    return len(set(lst))

def generate_random_set(size, start, end):
    import random
    return set(random.randint(start, end) for _ in range(size))
